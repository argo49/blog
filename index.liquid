<p>An endeavor by Tyler Argo.</p>
{% assign reversed_posts = collections.posts | reverse %}
{% for post in reversed_posts %}
    <h2><a href="{{ post.url }}">{{ post.data.pageTitle }}</a></h2>
    <time datetime="{{post.date}}">{{post.date}}</time>
{% endfor %}